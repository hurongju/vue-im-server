{
  "name": "vue-im-server",
  "version": "0.1.0",
  "author": "夜叉王的守护",
  "private": true,
  "description": "基于express的websocket服务端",
  "main": "src/app.js",
  "repository": {
		"type": "git",
		"url": "https://github.com/hurongju/vue-im-server"
  },
  "license":"MIT",
  "bugs": {
    "url" : "https://github.com/hurongju/vue-im-server/issues",
    "email" : "987063158@qq.com"
  },
  "scripts": {
    "start": "nodemon",
    "compile": "babel src -d dist",
    "server:stg": "NODE_ENV=staging node dist/app.js",
    "server:prd": "NODE_ENV=production node dist/app.js",
    "stg": "npm run compile && npm run server:stg",
    "prd": "npm run compile && npm run server:prd",
    "inspect": "nodemon --inspect src/app.js",
    "lint": "npm run lint:js ",
    "lint:js": "npm run lint:eslint src/",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "await-to-js": "^2.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "ioredis": "^4.17.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "log4js": "^6.3.0",
    "md5": "^2.2.1",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "socket.io": "^3.0.1",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^6.0.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2"
  }
}
